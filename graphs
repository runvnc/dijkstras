  1   2     3
a ->b - c ----- I

   3|  3|      i|

    D - E - G - H
      i   i   i

Finding A to D

function* findShortPath() {
  for (var edge of this.currentVertex.edges) { 
    vertex = edge.endVertex;
    if (!vertex.visited) { 
      distance = currentVertex.distance + edge.weight;
      if (distance < vertex.distance) {
        vertex.distance = distance;
      }
    }
  }

  this.currentVertex.markVisited();

  yield this.unvisited.reduce(
    (a,b) => a.distance < b.distance : a ? b);
  );
}



